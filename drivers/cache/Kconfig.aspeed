config CACHE_AST10X0
	bool "AST10X0 external cache driver"
	depends on SOC_SERIES_AST10X0
	select CACHE_HAS_DRIVER
	help
	  This option enables the CACHE driver for ASPEED AST10X0 series SOC.

config CACHE_AST26XX
	bool "AST26XX external cache driver"
	depends on SOC_SERIES_AST26XX
	select CACHE_HAS_DRIVER
	help
	  This option enables the CACHE driver for ASPEED AST26XX series SOC.