 * Application overlay for creating temperature sensor device instance
 */

/ {
	stm-temp {
		label = "DIE_TEMP";
		compatible = "st,stm32-temp";
		io-channels = <&adc1 16>;
		avgslope = <43>;
		v25 = <1430>;
		ntc;
		status = "okay";
	};
};

&adc1 {
	status = "okay";
};
