if BOARD_AST1030_EVB

config BOARD
	default "ast1030_evb"

choice SOC_AST10X0_SELECTION
default SOC_AST1030
endchoice

if USB

config USB_ASPEED
	default y

config USB_DEVICE_STACK
	default y

config USB_DEVICE_MANUFACTURER
	default "Aspeed"

endif # USB

endif # BOARD_AST1030_EVB
